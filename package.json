{
  "name": "oh-lucy-vscode",
  "displayName": "oh lucy",
  "description": "a re-upload of a soft but clear syntax theme",
  "version": "1.0.0",
  "publisher": "Hermitter",
  "repository": "https://github.com/hermitter/oh-lucy-vscode-theme",
  "scripts": {
    "build": "node --experimental-modules src/index.mjs",
    "release": "npm run build && git add dist && git commit -m build-theme && npm version patch & vsce publish && git push"
  },
  "engines": {
    "vscode": "^1.26.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "static/icon.png",
  "contributes": {
    "themes": [
      {
        "label": "oh-lucy",
        "uiTheme": "vs-dark",
        "path": "./dist/lucy.json"
      },
      {
        "label": "oh-lucy-evening",
        "uiTheme": "vs-dark",
        "path": "./dist/lucy-evening.json"
      }
    ]
  },
  "tags": [
    "soft",
    "clear",
    "pink",
    "dark theme",
    "color-theme",
    "monokai"
  ],
  "dependencies": {
    "chroma-js": "^2.1.0"
  },
  "devDependencies": {
    "vsce": "^2.7.0"
  }
}